{
  "name": "portofolio_db",
  "version": "1.0.0",
  "description": "Portfolio Backend API",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "setup": "npm install && npm run db:create && npm run migrate",
    "setup:fresh": "npm install && npm run db:create && npm run migrate:fresh",
    "db:create": "node -e \"require('./models').createDatabaseIfNotExists().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "db:status": "node scripts/migrate.js status",
    "db:seed": "node seeders/seed.js",
    "db:seed:manual": "node seeders/seed.js",
    "migrate": "node scripts/migrate.js migrate",
    "migrate:fresh": "node scripts/migrate.js fresh",
    "migrate:status": "node scripts/migrate.js status",
    "routes:analyze": "node analyze-routes.js",
    "routes:test": "node analyze-routes.js test"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "portfolio",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "backend"
  ],
  "author": "Your Name",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
